---
import BaseLayout from "../layouts/BaseLayout.astro";

const identity = {
    firstname: "Tom",
    country: "France",
    skills: ["html", "css", "JS"],
};

const pageTitle = "About";
---

<BaseLayout pageTitle={pageTitle}>
    <h2>Hello!</h2>
    <p>My name is {identity.firstname} and i live in {identity.country}.</p>
    <ul>
        {identity.skills.map((skill) => <li>{skill}</li>)}
    </ul>
</BaseLayout>
